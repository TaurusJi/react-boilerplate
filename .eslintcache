ey: \"title\",\n      dataIndex: \"title\",\n    },\n    {\n      title: \"操作\",\n      key: \"action\",\n      dataIndex: \"id\",\n      render: (id: string) => <EditUserLabel id={id} />,\n    },\n  ];\n\n  const dataSource = [\n    {\n      id: \"1\",\n      title: \"123\",\n    },\n    {\n      id: \"2\",\n      title: \"123\",\n    },\n  ];\n\n  function onClick() {\n    const index = list.findIndex((item) => item.age === \"4\");\n    if (index > -1) {\n      removeAt(index);\n    }\n  }\n\n  useEffect(() => {\n    ref.current = index;\n  });\n\n  return (\n    <UserCss>\n      <Table rowKey=\"id\" dataSource={dataSource} columns={columns} />\n      <p className=\"currentIndex\">当前的index是: {index}</p>\n      <p className=\"prevIndex\">上一个index是: {ref.current}</p>\n      <Button\n        style={{ marginLeft: 0 }}\n        onClick={() => setIndex((prev) => prev + 1)}\n      >\n        点击让index加1\n      </Button>\n      {list.map((item, index) => (\n        <div key={index}>{item.age}</div>\n      ))}\n      <Button onClick={onClick}>点击测试</Button>\n    </UserCss>\n  );\n};\n\nexport default User;\n","/Users/mac/Documents/个人项目/project/react-boilerplate/src/layouts/NormalLayout.tsx",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/context/GlobalModal/index.tsx",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/context/GlobalModal/Provider.tsx",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/config/routes.ts",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/components/AclRouter/utils/omitRouteRenderProperties.ts",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/components/AclRouter/DefaultLayout.tsx",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/pages/FAQ/index.tsx",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/pages/Unauthorized/index.tsx",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/utils/request/fetcher.ts",[],"/Users/mac/Documents/个人项目/project/react-boilerplate/src/co